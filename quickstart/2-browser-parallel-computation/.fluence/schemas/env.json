{
  "$id": "https://fluence.dev/schemas/env.yaml",
  "title": "env.yaml",
  "type": "object",
  "description": "Defines user project preferences",
  "properties": {
    "fluenceEnv": {
      "title": "Fluence environment",
      "description": "Fluence environment to connect to",
      "type": "string",
      "enum": [
        "kras",
        "testnet",
        "stage",
        "local",
        "custom"
      ],
      "nullable": true
    },
    "version": {
      "type": "number",
      "const": 0
    }
  },
  "required": [
    "version"
  ]
}
